(7, 'Vente-dédicace lundi 26: A. Hadj-Nacer, La Martingale Algérienne', 'vente-dedicace', '', '<p><strong>Lundi 26 septembre 2011 </strong>à partir de <strong>15h30</strong>, <strong>Abderrahmane Hadj-Nacer</strong> dédicacera la nouvelle édition de son ouvrage <strong>"La martingale algérienne"</strong>, une édition augmentée d''une postface inédite et d''une préface de Kamel Daoud. Retrouvez le au<strong> Sila</strong> - stand des<strong> éditions barzakh (A 30)</strong></p>', '', 0, 1, 0, 1, '2011-09-23 16:02:57', 64, '', '2011-09-28 08:35:55', 64, 64, '2012-10-24 09:15:06', '2011-09-23 16:02:57', '0000-00-00 00:00:00', '', '', 'show_title=\nlink_titles=\nshow_intro=\nshow_section=\nlink_section=\nshow_category=\nlink_category=\nshow_vote=\nshow_author=\nshow_create_date=\nshow_modify_date=\nshow_pdf_icon=\nshow_print_icon=\nshow_email_icon=\nlanguage=\nkeyref=\nreadmore=', 9, 0, 36, '', '', 0, 26, 'robots=\nauthor=', 0),
(8, 'Rencontre avec Abderrahmane Hadj-Nacer - Béjaia', 'qla-martingale-algerienneq-bejaia', '', '<p><img src="images/img_sites/actualite/hadj nacer daoud 16e sila.jpg" border="0" alt="A Hadj-Nacer, Meddi et daoud au 16ème Sila" title="A Hadj-Nacer, Meddi et daoud au 16ème Sila" width="470" /></p>\r\n<p><strong>Abderrahmane Hadj-Nacer</strong>, auteur de <strong>« La Martingale Algérienne »</strong>, sera à<strong> Béjaia le 15 octobre</strong> où il participera à un débat autour de son essai au <strong>Théâtre Régional de Béjaia</strong>. Prochainement, il prendra part à des rencontres dans les villes de <strong>Constantine</strong> et d’<strong>Oran</strong>.</p>', '', 0, 1, 0, 1, '2011-09-23 16:05:03', 64, '', '2011-10-02 19:35:15', 66, 64, '2013-09-09 13:20:04', '2011-09-23 16:05:03', '0000-00-00 00:00:00', '', '', 'show_title=\nlink_titles=\nshow_intro=\nshow_section=\nlink_section=\nshow_category=\nlink_category=\nshow_vote=\nshow_author=\nshow_create_date=\nshow_modify_date=\nshow_pdf_icon=\nshow_print_icon=\nshow_email_icon=\nlanguage=\nkeyref=\nreadmore=', 9, 0, 33, '', '', 0, 110, 'robots=\nauthor=', 0),
(9, 'Rencontre avec Yassin Temlali - Béjaia', 'chroniques-cine-litteraires-de-deux-guerres-bejaia', '', '<p><img src="images/img_sites/actualite/yassin temlali.jpg" border="0" alt="Y. Temlali" title="Y. Temlali" /></p>\r\n<p><strong>Yassin Temlali</strong>, auteur de <strong>« Algérie, Chroniques Ciné-Littéraires De Deux Guerres » </strong>participera au Café Littéraire du <strong>samedi 08 octobre</strong> au <strong>Théâtre Régional de Béjaia.</strong></p>', '', 0, 1, 0, 1, '2011-09-23 16:07:03', 64, '', '2011-10-02 12:23:33', 64, 64, '2012-10-22 10:07:46', '2011-09-23 16:07:03', '0000-00-00 00:00:00', '', '', 'show_title=\nlink_titles=\nshow_intro=\nshow_section=\nlink_section=\nshow_category=\nlink_category=\nshow_vote=\nshow_author=\nshow_create_date=\nshow_modify_date=\nshow_pdf_icon=\nshow_print_icon=\nshow_email_icon=\nlanguage=\nkeyref=\nreadmore=', 7, 0, 34, '', '', 0, 67, 'robots=\nauthor=', 98),
