(69, 'Lecture de l''espace oasien en vidéo', 'lecture-de-lespace-oasien-en-video', '', '<p><a href="https://www.youtube.com/watch?v=iqEmZPtqjMk">Cliquez ici pour découvrir la vidéo postface de l''essai " Lecture de l''espace oasien" de Nadir Marouf. <br /></a></p>', '', 0, 1, 0, 1, '2013-03-25 07:29:52', 64, '', '2013-03-25 07:36:25', 64, 0, '0000-00-00 00:00:00', '2013-03-25 07:29:52', '0000-00-00 00:00:00', '', '', 'show_title=\nlink_titles=\nshow_intro=\nshow_section=\nlink_section=\nshow_category=\nlink_category=\nshow_vote=\nshow_author=\nshow_create_date=\nshow_modify_date=\nshow_pdf_icon=\nshow_print_icon=\nshow_email_icon=\nlanguage=\nkeyref=\nreadmore=', 2, 0, 17, '', '', 0, 224, 'robots=\nauthor=', 117),
(70, 'LE MAGHREB À TRAVERS SES PLANTES Médecine et tradition', 'le-maghreb-a-travers-ses-plantes-medecine-et-tradition', '', '<p><span>Us et coutumes, traditions, croyance et savoir-faire  tissent la trame de ce livre utile et captivant. Soucieux de transmettre  ce savoir ancestral à la nouvelle génération, l’auteur répertorie  toutes les espèces d’arbres, de plantes et de condiments spécifiques au  Maghreb...</span></p>\r\n<p><a href="http://www.lesoirdalgerie.com/articles/2013/04/07/article.php?sid=147406&cid=16">Cliquez ici pour lire l''article paru dans Le Soir d''Algérie</a></p>', '', 1, 4, 0, 8, '2013-04-07 08:16:57', 64, '', '0000-00-00 00:00:00', 0, 0, '0000-00-00 00:00:00', '2013-04-07 08:16:57', '0000-00-00 00:00:00', '', '', 'show_title=\nlink_titles=\nshow_intro=\nshow_section=\nlink_section=\nshow_category=\nlink_category=\nshow_vote=\nshow_author=\nshow_create_date=\nshow_modify_date=\nshow_pdf_icon=\nshow_print_icon=\nshow_email_icon=\nlanguage=\nkeyref=\nreadmore=', 1, 0, 6, '', '', 0, 166, 'robots=\nauthor=', 118),
(72, 'Figures d’Abd El-Kader en personnage romanesque', 'figures-dabd-el-kader-en-personnage-romanesque', '', '<p><strong style="margin: 0px; padding: 0px; border: 0px; outline: 0px; font-size: 13px; font-family: Arial, Verdana, sans-serif; vertical-align: baseline; line-height: 16px; text-align: justify;">Une date et un événement de la vie de l’Emir Abd El-Kader - son exil - ont suffi à l’écrivain pour composer un récit formidable, oscillant entre la biographie, le document et la fiction. Histoire et petites histoires se croisent dans ce texte concis, juste et efficace...</strong></p>\r\n<p> </p>\r\n<p><strong style="margin: 0px; padding: 0px; border: 0px; outline: 0px; font-size: 13px; font-family: Arial, Verdana, sans-serif; vertical-align: baseline; line-height: 16px; text-align: justify;"><a href="http://www.liberte-algerie.com/culture/figures-d-abd-el-kader-en-personnage-romanesque-la-derniere-nuit-de-l-emir-d-abdelkader-djemai-200475">Cliquez ici pour lire la suite de l''article de Sarah Kharfi</a></strong></p>', '', 1, 4, 0, 8, '2013-06-05 09:21:18', 64, '', '2013-06-05 11:47:58', 64, 0, '0000-00-00 00:00:00', '2013-06-05 09:21:18', '0000-00-00 00:00:00', '', '', 'show_title=\nlink_titles=\nshow_intro=\nshow_section=\nlink_section=\nshow_category=\nlink_category=\nshow_vote=\nshow_author=\nshow_create_date=\nshow_modify_date=\nshow_pdf_icon=\nshow_print_icon=\nshow_email_icon=\nlanguage=\nkeyref=\nreadmore=', 2, 0, 5, '', '', 0, 113, 'robots=\nauthor=', 15),
(73, 'Habib Ayyoub lauréat aux escales littéraires du Sofitel', 'habib-ayyoub-laureat-aux-escales-litteraires-du-sofitel', '', '<p><img src="images/img_sites/actualite/ayyoub 72 dpi.jpg" border="0" /></p>\r\n<p>Habib Ayyoub auteur de, entre autres, <a href="index.php?option=com_parution&view=parution&id=19&Itemid=3"> <em>L''homme qui n''existait pas</em></a>, <a href="index.php?option=com_parution&view=parution&id=20&Itemid=3"><em>Le désert et après</em></a> et <a href="index.php?option=com_parution&view=parution&id=21&Itemid=3"><em>Le palestinie</em>n</a> parus aux éditions barzakh, a remporté le prix du meilleur roman (dans la catégorie romancier confirmé) pour son dernier ouvrage <a href="index.php?option=com_parution&view=parution&id=128&Itemid=3"><em>Le remonteur d''horlog</em>e</a>, sorti en septembre 2012. le prix a été remis dans le cadre des escales littéraires du Sofitel, organisées pour la première fois à Alger.</p>', '', 1, 1, 0, 4, '2013-06-05 10:54:58', 64, '', '2013-06-05 11:02:05', 64, 0, '0000-00-00 00:00:00', '2013-06-05 10:54:58', '2013-06-05 10:54:58', '', '', 'show_title=\nlink_titles=\nshow_intro=\nshow_section=\nlink_section=\nshow_category=\nlink_category=\nshow_vote=\nshow_author=\nshow_create_date=\nshow_modify_date=\nshow_pdf_icon=\nshow_print_icon=\nshow_email_icon=\nlanguage=\nkeyref=\nreadmore=', 2, 0, 1, '', '', 0, 0, 'robots=\nauthor=', 23),
(74, 'Habib Ayyoub lauréat aux escales littéraires du Sofitel', 'habib-ayyoub-laureat-du-prix-du-meilleur-roman', '', '<p><img src="images/img_sites/actualite/ayyoub 72 dpi.jpg" border="0" /></p>\r\n<p>Habib Ayyoub auteur de, entre autres, <a href="index.php?option=com_parution&view=parution&id=19&Itemid=3"> <em>L''homme qui n''existait pas</em></a>, <a href="index.php?option=com_parution&view=parution&id=20&Itemid=3">Le désert et après</a> et <a href="index.php?option=com_parution&view=parution&id=21&Itemid=3"><em>Le palestinie</em></a>n parus aux éditions barzakh, a remporté le prix du meilleur roman ( dans la catégorie romancier confirmé) pour son dernier ouvrage <a href="index.php?option=com_parution&view=parution&id=128&Itemid=3"><em>Le remonteur d''horlog</em>e</a>, sorti en septembre 2012. le prix a été remis dans le cadre des escales littéraires du Sofitel, organisée pour la première fois à Alger.</p>', '', 0, 1, 0, 1, '2013-06-05 11:03:10', 64, '', '2013-06-05 11:06:03', 64, 0, '0000-00-00 00:00:00', '2013-06-05 11:03:10', '0000-00-00 00:00:00', '', '', 'show_title=\nlink_titles=\nshow_intro=\nshow_section=\nlink_section=\nshow_category=\nlink_category=\nshow_vote=\nshow_author=\nshow_create_date=\nshow_modify_date=\nshow_pdf_icon=\nshow_print_icon=\nshow_email_icon=\nlanguage=\nkeyref=\nreadmore=', 2, 0, 15, '', '', 0, 304, 'robots=\nauthor=', 0),
